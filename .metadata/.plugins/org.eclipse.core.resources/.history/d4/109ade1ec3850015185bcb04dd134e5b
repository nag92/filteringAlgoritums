power = lambda x,y: x**y


def regress(y,x,weights):
		my_x = make_x(x)	
			
		temp1 = np.transpose(zip(*make_x(x)))* weights  #* weights * y
		print temp1
				
			
			
def make_x( data):
	m = []
	
	for x in data:
		temp = []
		for i in xrange(3):
			temp.append(x**i)
		#print temp
		m.append(temp)
	#print np.transpose(m)
	return np.transpose(m)





x = np.vstack([1,2,3])
y = np.vstack([1,2,3 ])
w = np.diag([1,1,1 ])
regress(y, x, w)
