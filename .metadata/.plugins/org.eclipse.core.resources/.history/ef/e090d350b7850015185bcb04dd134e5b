power = lambda x,y: x**y


def regress(y,x,weights):
		my_x = make_x(x)		
		temp1 = np.transpose(my_x )#* weights * my_x # *  np.transpose(my_x )* weights * y
		print temp1
				
			
			
def make_x( data):
	m = []
	for x in data:
		temp = []
		for y in xrange(3):
			temp.append(x**y)
			m.append(temp) 
			
	return np.transpose(m)




x = np.vstack([1,2,3 ])
y = np.vstack([1,2,3 ])
w = np.vstack([1,1,1 ])
regress(y, x, w)

